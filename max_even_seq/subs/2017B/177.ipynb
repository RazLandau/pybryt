    "def max_even_seq(n):\n",
    "\tcounter = 0\n",
    "\tmax_sequence = 0\n",
    "    \n",
    "\tfor num in str(n):\n",
    "\t\tif int(num) % 2 == 0 :\n",
    "\t\t\tcounter += 1\n",
    "\t\telse:\n",
    "\t\t\tcounter = 0\n",
    "\t\tif counter > max_sequence :\n",
    "\t\t\tmax_sequence = counter\n",
    "\t\n",
    "\treturn (max_sequence)\n",
