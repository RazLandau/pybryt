    "def max_even_seq(n):\n",
    "    n = str(n)\n",
    "    maxSeq = 0\n",
    "    currentSeq = 0\n",
    "    for dig in n:\n",
    "        if int(dig) % 2 == 0:\n",
    "            currentSeq += 1\n",
    "            maxSeq = max (maxSeq, currentSeq)\n",
    "        else:\n",
    "            currentSeq = 0\n",
    "    return maxSeq\n",
