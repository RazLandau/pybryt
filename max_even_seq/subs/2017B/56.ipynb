    "def max_even_seq(n):\n",
    "    maxseq = 0\n",
    "    length = 0\n",
    "    for digit in str(n):\n",
    "        if (int(digit)%2 == 0):\n",
    "            length += 1\n",
    "        else:\n",
    "            length = 0\n",
    "        if (length > maxseq):\n",
    "            maxseq = length\n",
    "    return maxseq\n",
