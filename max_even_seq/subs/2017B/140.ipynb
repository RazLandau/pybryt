    "def max_even_seq(n):\n",
    "    cnt = 0\n",
    "    lst = []\n",
    "    for i in str(n):\n",
    "        if int(i)%2 == 0:\n",
    "            cnt = cnt + 1\n",
    "            lst.append(cnt)\n",
    "        else:\n",
    "            cnt = cnt*0\n",
    "            lst.append(cnt)\n",
    "\n",
    "    return max(lst)\n",
    "\n",
    "\n"
   ]
