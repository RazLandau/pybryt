    "def max_even_seq(n):\n",
    "    x=0\n",
    "    maxx=0\n",
    "    while n>0:\n",
    "        if n%2==0:\n",
    "            x+=1\n",
    "        else:\n",
    "            maxx=max(maxx,x)\n",
    "            x=0\n",
    "        maxx=max(maxx,x)\n",
    "        n=int(n/10)\n",
    "    return(maxx)\n",
    "\n",
    "\n"
   ]
