    "def max_even_seq(n):\n",
    "    count = 0\n",
    "    max_count = 0\n",
    "\n",
    "    for i in str(n):\n",
    "        if int(i) % 2 == 0:\n",
    "            count += 1\n",
    "        else:\n",
    "            max_count = count\n",
    "            count = 0\n",
    "\n",
    "    return max(max_count, count)\n",
    "\n",
    "\n"
   ]
