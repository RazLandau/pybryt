    "def max_even_seq(n):\n",
    "    max = 0\n",
    "    cur = 0\n",
    "    for s in str(n):\n",
    "        if int(s) % 2 == 0:\n",
    "            cur += 1\n",
    "        else:\n",
    "            cur = 0\n",
    "        if (cur > max):\n",
    "            max = cur\n",
    "    return max\n",
    "\n",
    "\n"
   ]
