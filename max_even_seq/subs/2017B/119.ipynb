    "def max_even_seq(n):\n",
    "    max_len = 0\n",
    "    while n > 0:\n",
    "        if n%2 == 0:\n",
    "            cnt += 1\n",
    "            n = n//10\n",
    "            if cnt>max_len:\n",
    "                max_len=cnt\n",
    "        else:\n",
    "            cnt = 0\n",
    "            n = n//10\n",
    "    return(max_len)\n",
