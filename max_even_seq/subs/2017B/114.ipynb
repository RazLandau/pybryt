    "def max_even_seq(n):\n",
    "    maxpint = 0\n",
    "    pint = 0\n",
    "    for i in str(n):\n",
    "        if int(i)%2 == 0:\n",
    "            pint += 1\n",
    "            if maxpint < pint:\n",
    "                maxpint = pint\n",
    "        else:\n",
    "            pint = 0\n",
    "    return maxpint\n",
