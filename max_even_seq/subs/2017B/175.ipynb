    "def max_even_seq(n):\n",
    "    max=0\n",
    "    cnt=0\n",
    "    for dig in str(n):\n",
    "        if int(dig)%2==0:\n",
    "            cnt+=1\n",
    "            if cnt>max:\n",
    "                max=cnt\n",
    "        else:\n",
    "            cnt=0\n",
    "    return (max)\n",
    "\n",
