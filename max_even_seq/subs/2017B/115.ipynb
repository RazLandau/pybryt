   "source": [
    "def max_even_seq(n):\n",
    "\n",
    "    strn = str(n)\n",
    "    cnt = 0\n",
    "    cmpr = 0\n",
    "    for digit in strn:\n",
    "        if int(digit)%2 == 0:\n",
    "            cnt +=1\n",
    "        else:\n",
    "            cmpr = max(cnt, cmpr)\n",
    "            cnt = 0\n",
    "    cmpr = max(cnt, cmpr)\n",
    "    return cmpr\n",
    "\n",
    "\n"
   ]
  },
