   "source": [
    "def max_even_seq(n):\n",
    "    max_even = 0\n",
    "    temp=0\n",
    "    while n>0:\n",
    "        if(n%10)%2!=0:\n",
    "            temp=0\n",
    "        else:\n",
    "            temp = temp+1\n",
    "            if temp>max_even:\n",
    "                max_even=temp\n",
    "        n=n//10\n",
    "    return(max_even)\n"
   ]
