   "source": [
    "def max_even_seq(n):\n",
    "    longest, current = 0, 0\n",
    "    for evenp in map(lambda x: int(x)%2 == 0, str(n)):\n",
    "        if evenp:\n",
    "            current = current + 1\n",
    "        else:\n",
    "            current = 0\n",
    "        longest = max(longest, current)\n",
    "    return longest\n",
    "\n",
