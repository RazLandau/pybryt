    "def max_even_seq(n):\n",
    "    cnt = 0\n",
    "    num = n\n",
    "    max_seq = 0\n",
    "    while num>0:\n",
    "        if num%2==0:\n",
    "            cnt+=1\n",
    "        if cnt > max_seq:\n",
    "            max_seq = cnt\n",
    "        if num%2 != 0:\n",
    "            cnt = 0\n",
    "        num = num//10\n",
    "    return max_seq\n",
