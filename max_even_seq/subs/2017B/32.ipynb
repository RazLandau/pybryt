   "source": [
    "def max_even_seq(n):\n",
    "    snum = str(n)\n",
    "    max_seq = 0\n",
    "    cnt = 0\n",
    "\n",
    "    for digit in snum:\n",
    "        if int(digit) % 2 == 0:\n",
    "            cnt += 1\n",
    "        else:\n",
    "            max_seq = max(max_seq,cnt)\n",
    "            cnt = 0\n",
    "\n",
    "    return max(max_seq,cnt)\n",
    "\n",
    "\n"
   ]
