    "def max_even_seq(n):\n",
    "    cnt1=0\n",
    "    cnt2=0\n",
    "    for i in str(n):\n",
    "        if int(i) % 2 == 0:\n",
    "            cnt1 += 1\n",
    "            if cnt1 > cnt2:\n",
    "                cnt2 = cnt1\n",
    "        else :\n",
    "            cnt1=0\n",
    "\n",
    "    return cnt2\n",
    "\n"
   ]
