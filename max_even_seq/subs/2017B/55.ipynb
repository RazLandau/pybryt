    "    max_seq=[]\n",
    "    for i in str(n):\n",
    "      if int(i)%2 ==0:\n",
    "        long_seq=long_seq+1\n",
    "        max_seq.append(long_seq)  \n",
    "      else:\n",
    "        long_seq=0\n",
    "        max_seq.append(long_seq)  \n",
    "     \n",
    "    return (max(max_seq))\n",
