   "source": [
    "def max_even_seq(n):\n",
    "    high_score = 0\n",
    "    even_seq = 0\n",
    "    for c in str(n):\n",
    "        if int(c) % 2 == 0:\n",
    "            even_seq += 1\n",
    "        else:\n",
    "            high_score = max(high_score, even_seq)\n",
    "            even_seq = 0\n",
    "\n",
    "    return high_score\n",
    "\n"
   ]
