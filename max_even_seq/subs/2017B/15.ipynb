    "def max_even_seq(n):\n",
    "    maxCounter = 0\n",
    "    counter = 0\n",
    "    for digit in str(n):\n",
    "        isEven = int(digit) % 2 == 0\n",
    "        counter = (counter * isEven) + isEven * isEven\n",
    "        maxCounter = max(maxCounter, counter)\n",
    "    return maxCounter\n",
    "\n",
