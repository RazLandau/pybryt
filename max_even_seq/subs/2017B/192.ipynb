   "source": [
    "def max_even_seq(n):\n",
    "    cnt=0\n",
    "    maxcnt=0\n",
    "    while n > 0:\n",
    "        res = n % 10\n",
    "        n = n // 10\n",
    "        if res%2==0:\n",
    "            cnt+=1\n",
    "        else:\n",
    "            maxcnt=max(cnt,maxcnt)\n",
    "            cnt=0\n",
    "    return maxcnt\n",
    "\n",
    "\n",
    "\n"
   ]
