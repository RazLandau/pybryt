    "def max_even_seq(num):\n",
    "    snum=str(num)\n",
    "    curLen=0\n",
    "    maxLen=0\n",
    "    for dig in snum:\n",
    "        if int(dig)%2==0:\n",
    "            curLen+=1\n",
    "        else:\n",
    "            maxLen=curLen\n",
    "            curLen=0\n",
    "    return max(curLen,maxLen)\n"
   ]
