    "def max_even_seq(n):\n",
    "    cnt=0\n",
    "    mx=0\n",
    "    for i in str(n):\n",
    "        if (int(i)%2==0):\n",
    "            cnt=cnt+1\n",
    "        else:\n",
    "            cnt=0\n",
    "        if (cnt>mx):\n",
    "            mx=cnt\n",
    "    return mx\n",
