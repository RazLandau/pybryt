    "def max_even_seq(n):\n",
    "    strike = 0\n",
    "    basket=[]\n",
    "    for i in str(n):\n",
    "        if int(i)%2==0:\n",
    "            strike+=1\n",
    "            basket += [strike]\n",
    "        else:\n",
    "            strike = 0\n",
    "    basket.sort()\n",
    "    basket.reverse()\n",
    "    return (basket[0])\n"
   ]
