   "source": [
    "def max_even_seq(n):\n",
    "    cnt = 0\n",
    "    cnt_max = 0\n",
    "    ns = str(n)\n",
    "    for i in ns:\n",
    "        if int(i)%2 == 0:\n",
    "            cnt = cnt + 1\n",
    "            if cnt > cnt_max:\n",
    "                cnt_max = cnt\n",
    "        else:\n",
    "            cnt = 0\n",
    "    return (cnt_max)\n",
    "\n"
   ]
