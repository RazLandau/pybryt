   "source": [
    "def max_even_seq(n):\n",
    "    maxLen=0\n",
    "    cnt = 0\n",
    "    num=str(n)\n",
    "    for dig in num:\n",
    "        if int(dig)%2==0:\n",
    "            cnt = cnt + 1\n",
    "        else:\n",
    "            if cnt>maxLen:\n",
    "                maxLen = cnt\n",
    "            cnt = 0\n",
    "        \n",
    "    return(maxLen)\n",
    "         \n"
   ]
