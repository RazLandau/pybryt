    "def max_even_seq(n):\n",
    "    cnt = 0\n",
    "    max_cnt = 0\n",
    "    num = str(n)\n",
    "    for i in num:\n",
    "        if int(i) % 2 == 0:\n",
    "            cnt += 1\n",
    "            if cnt > max_cnt:\n",
    "                max_cnt = cnt\n",
    "        else:\n",
    "            cnt = 0\n",
    "            \n",
    "    return max_cnt\n",
