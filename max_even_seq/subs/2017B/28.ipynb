    "def max_even_seq(n):\n",
    "    maxseq=0\n",
    "    cnt=0\n",
    "    for dig in str(n):\n",
    "        if int(dig)%2==0:\n",
    "            cnt+=1\n",
    "        else:\n",
    "            if cnt>maxseq:\n",
    "                maxseq=cnt\n",
    "            cnt=0\n",
    "    if cnt>maxseq:\n",
    "        maxseq=cnt\n",
    "    return maxseq\n",
    "\n",
    "\n"
   ]
