    "def max_even_seq(n):\n",
    "    c=0\n",
    "    m=0\n",
    "    while n>0:\n",
    "        if (n%10)%2==0:\n",
    "            c+=1\n",
    "        else:\n",
    "            if c>m:\n",
    "                m=c\n",
    "            c=0\n",
    "        n//=10\n",
    "    if c>m:\n",
    "        m=c\n",
    "    return(m)\n",
    "\n"
   ]
