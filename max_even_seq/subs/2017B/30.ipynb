    "def max_even_seq(n):\n",
    "    nStr = str(n)\n",
    "    cnt = 0\n",
    "    maxEven = 0\n",
    "    for dig in nStr:\n",
    "        if int(dig)%2!=0:\n",
    "            cnt=0\n",
    "        else:\n",
    "            cnt += 1\n",
    "            maxEven = max(cnt, maxEven)\n",
    "    return (maxEven)\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
