    "def max_even_seq(n):\n",
    "    evenseq = []\n",
    "    d = 0\n",
    "    while n>0:\n",
    "        while n%2 == 0 and n>0:\n",
    "            d += 1\n",
    "            n = n//10\n",
    "        evenseq.append(d)\n",
    "        d = 0\n",
    "        n = n//10\n",
    "    return max(evenseq)\n",
