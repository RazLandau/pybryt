    "def max_div_seq(n, k):\n",
    "    maxi=0\n",
    "    c=0\n",
    "    lst=list(str(n))\n",
    "    for i in range(len(lst)):\n",
    "        if int(lst[i])%k==0:\n",
    "            c+=1\n",
    "        else:\n",
    "            if c>maxi:\n",
    "                maxi=c\n",
    "            c=0\n",
    "    if c>maxi:\n",
    "        maxi=c\n",
    "    return maxi"
   ]
