   "source": [
    "def max_div_seq(n, k):\n",
    "    cnt_chk = 0\n",
    "    cnt_max = 0\n",
    "    n_to_str = str(n)\n",
    "    for i in n_to_str:\n",
    "        if int(i) % k == 0:\n",
    "            cnt_chk += 1\n",
    "            cnt_max = max(cnt_chk, cnt_max)\n",
    "        else:\n",
    "            cnt_chk = 0\n",
    "\n",
    "    return cnt_max"
   ]
