    "def max_div_seq(n,k):\n",
    "    str_n = str(n)\n",
    "    max_div = []\n",
    "    i = 0\n",
    "    for num in str_n:\n",
    "        if int(num) % k == 0:\n",
    "            i += 1\n",
    "        else:\n",
    "            max_div.append(i)\n",
    "            i = 0\n",
    "    max_div.append(i)\n",
    "    return (max(max_div))"
   ]
